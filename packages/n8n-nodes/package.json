{
  "name": "n8n-nodes-papra",
  "version": "0.1.0",
  "description": "n8n nodes for Papra, the document archiving platform.",
  "author": "Corentin Thomasset <corentinth@proton.me> (https://corentin.tech)",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
